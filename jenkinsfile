pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('pwd03') {
            steps {
                sh pwd
            }
        }
	stage('pull source code from githubt') {
            steps {
		sh 'cd /Users/longnguyen/test01'
		sh 'git pull origin main' 
            }

    }
}
    
}
